<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainActivity">

    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.eater.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" >
        <action
            android:id="@+id/action_mainActivity_to_registrarUsuario"
            app:destination="@id/registrarUsuario"
            app:enterAnim="@anim/nav_default_enter_anim" />
    </activity>
    <activity
        android:id="@+id/registrarUsuario"
        android:name="com.example.eater.registrarUsuario"
        android:label="activity_registrar_usuario"
        tools:layout="@layout/activity_registrar_usuario" />
</navigation>